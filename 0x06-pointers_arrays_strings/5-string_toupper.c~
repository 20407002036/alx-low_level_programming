#include "main.h"
#include <stdio.h>
/**
 * string_toupper - changes lowercase letters to upper case
 *
 * @ :pointer to the string
 *
 * Return: Char
 */
char *string_toupper(char *str)
{

	while (*str != '\0')
	{
		if (*str >= 'a' && *str <= 'z')
		{
			*str  = *str - 'a' + 'A';
		}
		str++;

       }

	return (str);
}
