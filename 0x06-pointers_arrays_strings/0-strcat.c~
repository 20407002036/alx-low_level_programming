#include "main.h"
#include <stdio.h>
/**
 * _strcat - concatinates two stings
 *
 * @scr: string to be added to the dest
 * @dest: stding to be added
 *
 * Return: char
 */
char *_strcat(char *dest, char *src)
{
	int length = 0;

	while (*dest != '\0')
	{
		length++;
		dest++;
	}
	while (*src != '\0')
	{
		*dest = *src;
		dest++;
		src++;
	}

	*dest = '\0';

	return (dest);
}
